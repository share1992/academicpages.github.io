issn = {15237052},
journal = {Organic Letters},
number = {22},
pages = {6212--6215},
pmid = {29115843},
title = {{Synthesis of Highly Stereodefined Tetrasubstituted Acyclic All-Carbon Olefins via a Syn-Elimination Approach}},
volume = {19},
year = {2017}
}
