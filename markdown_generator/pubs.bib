issn = {15205126},
journal = {Journal of the American Chemical Society},
number = {29},
pages = {9291--9298},
pmid = {29968475},
title = {{Rearrangement of Hydroxylated Pinene Derivatives to Fenchone-Type Frameworks: Computational Evidence for Dynamically-Controlled Selectivity}},
volume = {140},
year = {2018}
}
